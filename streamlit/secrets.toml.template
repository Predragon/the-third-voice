# .streamlit/secrets.toml.template
# This file is a template for your Streamlit secrets.toml file.
# Rename it to secrets.toml and fill in your actual credentials.

[openrouter]
api_key = "your_openrouter_api_key_here" # Get from openrouter.ai

[supabase]
url = "your_supabase_url_here" # Get from Supabase project settings -> API
key = "your_supabase_anon_key_here" # Get from Supabase project settings -> API

[resend]
api_key = "your_resend_api_key_here" # For email verification/notifications, if used

# You can add other models or configurations here if needed
[MODELS]
model1 = "google/gemma-2-9b-it:free"
model2 = "meta-llama/llama-3.2-3b-instruct:free"
# Add any other models you plan to support
